package fr.pizzeria.model.menu;

//     fr.pizzeria.model.Pizza;
import fr.pizzeria.model.Pizza.Pizza;

// Importation de librairie de liste
import java.util.List;

public abstract class MenuAbstract {
	private Integer nbMenu;
	private List  menu;
	
	public MenuAbstract(){
		nbMenu++;
	}
	public String recupererNomMenu();
	public abstract void retournerAffichage();
	public abstract void action(Pizza[] tab);
		
}

