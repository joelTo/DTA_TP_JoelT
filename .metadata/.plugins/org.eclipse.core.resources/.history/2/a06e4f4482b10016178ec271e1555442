package fr.pizzeria.model;

import fr.pizzeria.console.PizzeriaAdminConsoleApp.*;

public class Pizza {

	Integer id;
	String code;
	String nom;
	Double prix;
	static Integer nbPizzas;


	public Pizza(Integer id, String code, String nom, Double prix){
		this.id=id;
		this.code=code; 
		this.nom=nom;
		this.prix=prix;
	}	


	
	
}

